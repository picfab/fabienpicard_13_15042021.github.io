'react-router-dom'\nimport { useDispatch, useSelector } from 'react-redux'\n\nimport { logoutUserAction } from '../store/userActions'\nimport { userSelector } from '../store/userSelectors'\nimport argentBankLogo from '../img/argentBankLogo.png'\n/**\n * Create a Navigation section\n * @module Footer\n * @component\n * @example\n * return (\n *   <Nav />\n * )\n */\nexport default function Nav() {\n  const history = useHistory()\n  const location = useLocation()\n  const user = useSelector(userSelector)\n  const dispatch = useDispatch()\n  const logout = (e) => {\n    e.preventDefault()\n    dispatch(logoutUserAction())\n    localStorage.removeItem('token')\n    localStorage.removeItem('lastConnection')\n    history.push('/')\n  }\n  return (\n    <nav className='main-nav'>\n      <Link className='main-nav-logo' to='/'>\n        <img\n          className='main-nav-logo-image'\n          src={argentBankLogo}\n          alt='Argent Bank Logo'\n        />\n        <h1 className='sr-only'>Argent Bank</h1>\n      </Link>\n      <div>\n        {!user.token ? (\n          <Link className='main-nav-item' to='/signin/'>\n            <FontAwesomeIcon icon={faUserCircle} />\n            Sign In\n          </Link>\n        ) : (\n          <>\n            {location.pathname !== '/profile' && user.data && (\n              <Link className='main-nav-item' to='/profile/'>\n                <FontAwesomeIcon icon={faUserCircle} />\n                {user.data.firstName}\n              </Link>\n            )}\n            <span\n              role='button'\n              tabIndex='0'\n              onKeyDown={logout}\n              className='main-nav-item'\n              onClick={logout}>\n              <FontAwesomeIcon icon={faSignOutAlt} />\n              Sign Out\n            </span>\n          </>\n        )}\n      </div>\n    </nav>\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"151bb0721c1f085fb3d9af95229dba43","cacheData":{"env":{}}}